{
    "manifest_version": 2,
    "name": "June",
    "description": "Batch download files on a webpage",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "64": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "css": ["style.css"],
            "js": [
                "FileSaver.min.js",
                "jszip.min.js",
                "jszip-utils.min.js",
                "jquery-3.5.1.min.js",
                "contentScript.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "Uni-Sans-Light_0.otf",
        "uni-sans-light-italic-webfont.ttf",
        "uni-sans-semibold-italic-webfont.ttf",
        "toolbar.html",
        "style.css"
    ]
}